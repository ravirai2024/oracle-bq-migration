{
  "name": "bigquery-sink-connector",
  "config": {
    "connector.class": "com.wepay.kafka.connect.bigquery.BigQuerySinkConnector",
    "tasks.max": "1",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "io.confluent.connect.avro.AvroConverter",
    "transforms": "InsertTimestamp,RenameTopic",
    "errors.tolerance": "all",
    "errors.log.enable": "true",
    "errors.log.include.messages": "true",
    "topics.regex": "C__MYUSER\\..*",
    "errors.deadletterqueue.topic.name": "BQSINK.DLT.TOPIC",
    "errors.deadletterqueue.context.headers.enable": "true",
    "transforms.InsertTimestamp.type": "org.apache.kafka.connect.transforms.InsertField$Value",
    "transforms.InsertTimestamp.timestamp.field": "timestamp",
    "transforms.RenameTopic.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.RenameTopic.regex": "C__MYUSER\\.(.*)",
    "transforms.RenameTopic.replacement": "$1",
    "project": "gcpdebeziumpoc",
    "defaultDataset": "test_dataset",
    "schemaRetriever": "com.wepay.kafka.connect.bigquery.retrieve.IdentitySchemaRetriever",
    "keyfile": "/etc/kafka/connect-secrets/gcpdebeziumpoc-6f7b3ea3a4d3.json",
    "autoCreateTables": "true",
    "datasets": ".*=test_dataset",
    "schema.registry.url": "http://schema-registry:8081",
    "autoUpdateSchemas": "true",
    "key.converter.schemas.enable": "true",
    "value.converter.schema.registry.url": "http://schema-registry:8081",
    "value.converter.schemas.enable": "true",
    "upsertEnabled":"true",
    "upsertKeys":"ID",
    "kafkaKeyFieldName":"ID" 
  }
}