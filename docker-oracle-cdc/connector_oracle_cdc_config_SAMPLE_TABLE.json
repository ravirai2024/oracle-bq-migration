{
  "name": "oracle_cdc_sample_table",
  "config": {
    "connector.class": "io.confluent.connect.oracle.cdc.OracleCdcSourceConnector",
    "tasks.max": "1",
    "topic.creation.groups": "redo",
    "oracle.server": "oracle",
    "oracle.port": "1521",
    "oracle.sid": "ORCLCDB",
    "oracle.username": "C##MYUSER",
    "oracle.password": "password",
    "start.from": "snapshot",
    "key.converter":"org.apache.kafka.connect.json.JsonConverter",
    "value.converter":"org.apache.kafka.connect.json.JsonConverter",
    "key.converter.schemas.enable":"true",
    "value.converter.schemas.enable":"true",
    "redo.log.corruption.topic": "redo-corruption-topic",
    "redo.log.consumer.bootstrap.servers": "broker:29092",
    "key.template": "${primaryKeyStructOrValue}",
    "table.inclusion.regex": "ORCLCDB\\.C##MYUSER\\.SAMPLE_TABLE",
    "table.exclusion.regex": "",
    "table.topic.name.template": "${schemaName}.${tableName}",
    "connection.pool.max.size": "20",
    "confluent.topic.bootstrap.servers": "broker:29092",
    "confluent.topic.replication.factor": "1",
    "topic.creation.default.partitions": "1",
    "topic.creation.redo.partitions": "1",
    "topic.creation.redo.include": "redo-log-topic",
    "topic.creation.default.replication.factor": "1",
    "topic.creation.redo.retention.ms": "1209600000",
    "topic.creation.redo.replication.factor": "1",
    "topic.creation.default.cleanup.policy": "delete",
    "topic.creation.redo.cleanup.policy": "delete"
  }
}